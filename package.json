{
  "name": "@bsramin/slack-jenkins-bot",
  "version": "1.0.0",
  "description": "Slack-Jenkins Bot",
  "license": "MIT",
  "authors": [
    "Ramin Banihashemi <a@ramin.it> (https://github.com/bsramin)"
  ],
  "main": "dist/src/index.js",
  "_moduleAliases": {
    "@app": "dist/src"
  },
  "engines": {
    "node": "14.x.x",
    "npm": "7.x.x"
  },
  "scripts": {
    "dev": "ENVK=.env.dev nodemon --ignore 'src/**/*.test.ts' -e ts,njk,md,json --exec node -r envk -r ts-node/register -r tsconfig-paths/register ./src/server.ts",
    "prod": "ENVK=.env.prod node -r module-alias/register -r envk ./dist/src/server.js",
    "tsc": "tsc",
    "compile": "tsc -p . --skipLibCheck"
  },
  "dependencies": {
    "envk": "^2.0.1",
    "koa": "^2.13.1",
    "koa-helmet": "^6.1.0",
    "koa-pino-logger": "^3.0.0",
    "koa-router": "^10.0.0",
    "module-alias": "^2.2.2",
    "mysql2": "^2.2.5",
    "pino": "^6.11.2"
  },
  "devDependencies": {
    "@types/koa": "^2.13.1",
    "@types/koa-helmet": "^6.0.2",
    "@types/koa-pino-logger": "^3.0.0",
    "@types/koa-router": "^7.4.1",
    "@types/mysql2": "types/mysql2",
    "@types/node": "^14.14.35",
    "@types/pino": "^6.3.6",
    "nodemon": "^2.0.7",
    "pino-pretty": "^4.7.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  }
}
